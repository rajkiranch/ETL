<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
    <section name="fileElementSection" type="SysproIntegration.Library.Configuration.FileElementSection,SysproIntegration.Library"/>
    <section name="serviceElementSection" type="SysproIntegration.Library.Configuration.ServiceElementSection,SysproIntegration.Library"/>
  </configSections>
  <fileElementSection>
    <fileElements>
      <add key="DefaultQuickBooksFileConnection"  path="C:\QBDataFile\qb file\Contoso.qbw" flag="No" maxrecords="10" value="QBToAcumatica"></add>
    </fileElements>
  </fileElementSection>
  <serviceElementSection>
    <serviceElements>
      <add key="DefaultAcumaticaServiceConnection"   maxrecords="10" username="Admin@SysproPune" flag="No" password="shri75024" url="http://192.168.12.28/AcumaticaERP/Soap/ACUMATICASERVI5.asmx"></add>
    </serviceElements>
  </serviceElementSection>
  <appSettings>
    <add key="log4net.Internal.Debug" value="true"/>
  </appSettings>
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender,log4net">
      <file type="log4net.Util.PatternString" value="C:\SysProLogs\SyproErrorLog.%date{MMddyyyy}.log.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="MMddyyyy"/>
      <rollingStyle value="Date"/>
      <staticLogFileName value="false"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <header value="[Header] "/>
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
        <footer value="[Footer] "/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO"/>
        <levelMax value="FATAL"/>
      </filter>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="FileAppender"/>
    </root>
  </log4net>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>